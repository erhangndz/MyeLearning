@using Entity.Models;
@model Message
@{
    ViewData["Title"] = "MessageDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row page-titles">
    <div class="col-md-5 col-8 align-self-center">
        <h3 class="text-themecolor m-b-0 m-t-0">Mesaj İçeriği</h3>

    </div>

</div>

<div class="col-lg-12">
    <div class="card">
        <div class="card-block">


            <form method="get">
                <input type="hidden" asp-for="MessageId" />
                <label>Ad Soyad</label>
                <br />
                <input type="text" asp-for="Name" class="form-control" disabled/>
                <br />
                <label>Konu</label>
                <br />
                <input type="text" asp-for="Subject" class="form-control" disabled />
                <br />
                <label>Mesaj</label>
                <br />
                <textarea type="text" asp-for="MessageContent" rows="10" class="form-control" disabled></textarea>
                <br />
                <br />

               
            </form>



        </div>
    </div>
</div>

