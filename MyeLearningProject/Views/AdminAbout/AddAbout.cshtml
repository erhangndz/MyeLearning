@using Entity.Models;
@model AboutUs
@{
    ViewData["Title"] = "AddAbout";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<div class="row page-titles">
    <div class="col-md-5 col-8 align-self-center">
        <h3 class="text-themecolor m-b-0 m-t-0">Hakkımızda Ekleme Sayfası</h3>

    </div>

</div>

<div class="col-lg-12">
    <div class="card">
        <div class="card-block">


            <form method="post">

                <label>Başlık</label>
                <br />
                <input type="text" asp-for="Title" class="form-control" />
                <br />
                <label>Açıklama</label>
                <br />
                <input type="text" asp-for="Description" class="form-control" />
                <br />
                <label>Görsel Url</label>
                <br />
                <input type="text" asp-for="Image" class="form-control" />
                <br />
                <label>Madde</label>
                <br />
                <input type="text" asp-for="Item" class="form-control" />
                <br />
                <br />
                <button class="btn btn-outline-primary">Kaydet</button>
            </form>



        </div>
    </div>
</div>